    "android:preprod": "npm run env:preprod && npm run _android",
    "android:prod": "npm run env:prod && npm run _android",
    "android:clean": "cd android && gradlew clean && cd ..",
    "_ios":"react-native run-ios",
    "ios": "npm run env:dev && npm run _ios",
    "ios:staging": "npm run env:staging && npm run _ios",
    "ios:stagingBeta": "npm run env:stagingBeta && npm run _ios",
    "ios:preprod": "npm run env:preprod && npm run _ios",
