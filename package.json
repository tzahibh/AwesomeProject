    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^9.3.0",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.3.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
