  "private": true,
  "scripts": {
    "env:dev": "node scripts/set-environment.js development",
    "env:staging": "node scripts/set-environment.js staging",
    "env:stagingBeta": "node /scripts/set-environment.js stagingBeta",
    "env:preprod": "node scripts/set-environment.js preprod",
    "env:prod": "node scripts/set-environment.js prod",
